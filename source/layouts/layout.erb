<%= partial "/layouts/partials/head" %>
  
  <body>


    <%= yield %>


    <%= javascript_include_tag  "all" %>
    
    <script src="http://maps.google.com/maps/api/js?sensor=false&amp;language=en"></script>
    <script>
    $(function() {
        $('#sti-menu').iconmenu({
          animMouseenter  : {
            'headtext' : {speed : 400, easing : 'easeOutBack', delay : 200, dir : -1},
            'stext' : {speed : 400, easing : 'easeOutBack', delay : 400, dir : -1},
            'icon'  : {speed : 400, easing : 'easeOutBack', delay : 0, dir : -1}
          },
          animMouseleave  : {
            'headtext' : {speed : 200, easing : 'easeInExpo', delay : 150, dir : 1},
            'stext' : {speed : 200, easing : 'easeInExpo', delay : 300, dir : 1},
            'icon'  : {speed : 200, easing : 'easeInExpo', delay : 0, dir : 1}
          }
        });
      });
    </script>
  </body>
</html>
